#!/bin/bash

warning="# WARNING: THIS FILE IS AUTOMATICALLY GENERATED."

echo $warning > variants_sieve.gnuplot
echo $warning > variants_enum.gnuplot

sed 's/xxxx/sieve/g' variants_template.gnuplot >> variants_sieve.gnuplot
sed 's/xxxx/enum/g' variants_template.gnuplot >> variants_enum.gnuplot

gnuplot variants_sieve.gnuplot
gnuplot variants_enum.gnuplot
